{
  "name": "@cderamos-2ct/n8n-nodes-custom",
  "version": "1.0.0",
  "description": "Custom n8n nodes for OnPrintShop, MightyCall, and XPS Ship",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "onprintshop",
    "mightycall",
    "xps",
    "shipping",
    "phone"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "cderamos-2ct",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cderamos-2ct/n8n-nodes-custom"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons && gulp build:credential-icons",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/OnPrintShopApi.credentials.js",
      "dist/credentials/MightyCallApi.credentials.js",
      "dist/credentials/XPSApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/OnPrintShop/OnPrintShop.node.js",
      "dist/nodes/MightyCall/MightyCall.node.js",
      "dist/nodes/XPSShip/XPS.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "gulp": "^4.0.2",
    "typescript": "^4.9.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "n8n-workflow": "^1.0.0"
  }
}

